#%RAML 1.0 Trait

usage: Apply this to any method that needs to be secured
headers: 
  client_id:
    description: |
      Security token to access api on Anypoint Platform
    displayName: Client ID
    type: string
    example: 3ddbd94c96c04aa2a0febb7603e12r4c2
    required: true  
  client_secret:
    description: |
      Security token to access api on Anypoint Platform
    displayName: Client Secret
    type: string
    example: 3ddbd94c96c04aa2a0febb7603e12r4c2
    required: true
